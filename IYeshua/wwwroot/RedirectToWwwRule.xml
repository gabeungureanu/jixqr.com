<rewrite>
  <rules>
    <rule name="Redirect to www" stopProcessing="true">
      <match url="(.*)" />
      <conditions>
        <add input="{HTTP_HOST}" pattern="^askjubileegpt\.com$" ignoreCase="true" />
      </conditions>
      <action type="Redirect" url="https://www.askjubileegpt.com/{R:1}" redirectType="Permanent" />
    </rule>
  </rules>
</rewrite>
